{
  "name": "@cardinal/rewards-center",
  "version": "1.10.7",
  "description": "Cardinal rewards center",
  "keywords": [
    "solana",
    "anchor",
    "cardinal"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "git@github.com:cardinal-labs/cardinal-rewards-center.git",
  "author": "Jeremy Bogle <jeremy@cardinal.so>",
  "bugs": {
    "url": "https://github.com/cardinal-labs/cardinal-rewards-center/issues",
    "email": "team@cardinal.so"
  },
  "homepage": "https://cardinal.so",
  "license": "AGPL-3.0",
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,json,jsx,html,css,md}": "prettier --write"
  },
  "files": [
    "dist/",
    "sdk/"
  ],
  "scripts": {
    "build": "rm -fr dist/ && tsc -P tsconfig.cjs.json && tsc -P tsconfig.esm.json",
    "clean": "rm -fr dist/",
    "idl:generate": "./generate-idls.sh",
    "sdk:generate": "node generate-sdk.js",
    "typecheck": "tsc",
    "lint": "eslint 'sdk/**/*.{js,ts}' --fix",
    "test": "jest --setupFiles dotenv/config",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ src/index.ts"
  },
  "dependencies": {
    "@cardinal/common": "^2.0.11",
    "@cardinal/payment-manager": "^1.7.9",
    "@cardinal/token-manager": "^1.7.9",
    "@metaplex-foundation/mpl-token-metadata": "^1.2.5",
    "@metaplex-foundation/rustbin": "^0.3.1",
    "@metaplex-foundation/solita": "^0.12.2"
  },
  "devDependencies": {
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jest": "^29.2.0",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.3",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4"
  }
}
